# JWT
# Must be a Base64-encoded string of at least 32 bytes (44 characters when encoded).
# Example (do NOT use this in production): JWT_SECRET=VGhpcy1pcy1hLXNlY3JldC1rZXktdGhhdC1pcy1sb25nLg==
JWT_SECRET=your-base64-encoded-jwt-secret
JWT_EXPIRATION_MS=your-expiration-time

# Database
DB_HOST=mysql
DB_PORT=3306
DB_NAME=your-db-name
DB_USERNAME=username
DB_PASSWORD=your-password

# AWS Credentials
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1
AWS_BUCKET_NAME=your-bucket-name